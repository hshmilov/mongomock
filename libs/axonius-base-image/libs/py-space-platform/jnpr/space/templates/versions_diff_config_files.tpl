{#
  Parameters used:
     - href1
         Href to one config file version that needs to be compared.
     - href2
         Href to the other config file version that needs to be compared.

#}
<versions-diff>
  <config-file-versions>
    <config-file-version href= "{{href1}}"/>
    <config-file-version href= "{{href2}}"/>
  </config-file-versions>
</versions-diff>