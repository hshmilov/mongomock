{
    "compilerOptions": {
      "target": "es2017",
      "allowSyntheticDefaultImports": false,
      "module": "es6"
    },
    "exclude": ["node_modules"],
    "baseUrl": "./",
    "paths": {
        "@": ["src/*"],
        "@components/*": ["src/components/*"],
        "@axons/*": ["src/components/axons/*"],
        "@networks/*": ["src/components/networks/*"],
        "@neurons/*": ["src/components/neurons/*"],
        "@pages/*": ["src/components/pages/*"],
        "@api/*": ["src/api/*"],
        "@store/*": ["src/store/*"],
    }
  }