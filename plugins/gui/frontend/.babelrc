{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "node": "current",
      },
    }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "syntax-dynamic-import",
    "transform-vue-jsx", 
    ["import", {
      "libraryName": "ant-design-vue",
      "libraryDirectory": "es",
      "style": true
    }]
  ]
}
